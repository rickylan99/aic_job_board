<div class="subjects index">
  <h2>Users</h2>

  <%= link_to("Add New User", new_user_path, :class => 'action new') %>

  <p><%= @to_print %></p>
  <table class="listing" summary="User list">
    <tr class="header">
      <th>Title</th>
      <th>Genre</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td class="actions">
        <%= link_to("Show", user_path(user), :class => 'action show') %>
        <%= link_to("Edit", edit_user_path(user), :class => 'action edit') %>
        <%= link_to("Delete", delete_user_path(user), :class => 'action delete') %>
      </td>
    </tr>
    <% end %>
  </table>
</div> 